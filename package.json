{
  "name": "@nsis/nlf",
  "version": "0.7.1",
  "description": "Parser and stringifier for NSIS Language Files",
  "license": "MIT",
  "scripts": {
    "build": "rollup --config",
    "dev": "npm run start",
    "lint": "eslint --fix ./src",
    "start": "rollup --watch --config",
    "test": "ava ./test/default.js --verbose"
  },
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/node-nlf.git"
  },
  "keywords": [
    "nsis",
    "nsis language file",
    "converter"
  ],
  "devDependencies": {
    "@rollup/plugin-typescript": "^6.1.0",
    "@types/node": "^14.0.14",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "ava": "^3.9.0",
    "eslint": "^7.4.0",
    "esm": "^3.2.25",
    "glob": "^7.1.6",
    "husky": "^4.2.5",
    "lint-staged": "^10.5.1",
    "rollup": "^2.18.2",
    "typescript": "^4.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "ava": {
    "require": [
      "esm"
    ]
  },
  "dependencies": {
    "json5": "^2.1.3"
  },
  "lint-staged": {
    "./src/*.ts": "eslint --cache --fix"
  }
}
